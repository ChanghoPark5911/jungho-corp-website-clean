{
  "name": "jungho-corp-website",
  "version": "0.1.0",
  "homepage": "https://jungho-corp-website-vercel.app",
  "dependencies": {
    "cors": "^2.8.5",
    "cra-template-typescript": "1.3.0",
    "express": "^5.1.0",
    "firebase": "^12.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/preset-create-react-app": "^9.1.1",
    "@storybook/react-webpack5": "^9.1.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.16",
    "eslint-plugin-storybook": "^9.1.1",
    "jest": "^27.5.1",
    "postcss": "^8.4.32",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.1",
    "tailwindcss": "^3.4.0",
    "webpack": "^5.101.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "start": "set TSC_COMPILE_ON_ERROR=true && set FAST_REFRESH=true && react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "npm run build && vercel --prod --yes",
    "deploy:preview": "npm run build && vercel --yes",
    "vercel-build": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
